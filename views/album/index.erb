<% @album.each do |album| %>
<div class="album-container">
<ul>
 <p><div id="album-item">Album Title: <%= album.title%></div></p>
<p><div id="album-item">Artist:<%= album.artist%></div></p>
<p><div id="album-item">Genre:<%= album.genre %></div></p>
<p><div id="album-item">Quantity in Stock:<%= album.quantity%></div></p>
<hr/></ul>
</div>

<% end %>